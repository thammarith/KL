	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"format": "pnpm prettier && pnpm lint",
		"lint": "eslint .",
		"prettier": "prettier --write --ignore-unknown .",
		"preview": "vite preview",
		"version:bump": "node -e \"const pkg=require('./package.json'); const now=new Date(); pkg.version=pkg.version.split('.').slice(0,2).join('.')+'.'+(Date.now()%1000000); pkg.buildDate=now.toISOString(); require('fs').writeFileSync('./package.json', JSON.stringify(pkg, null, '\\t')+require('os').EOL);\"",
		"deploy:functions": "cd functions && pnpm build && firebase deploy --only functions",
		"deploy:hosting": "pnpm version:bump && pnpm build && firebase deploy --only hosting",
		"deploy:all": "pnpm version:bump && pnpm build && pnpm deploy:functions && firebase deploy --only hosting",
		"serve:functions": "cd functions && pnpm serve",
		"serve:hosting": "firebase serve --only hosting",
		"logs:functions": "firebase functions:log"
	},
